{
    "openapi": "3.0.0",
    "info": {
        "title": "Api Agenda Documentacao", 
        "description": "Api Agenda",
        "version": "1.5",
        "contact": {
            "email": "amandalfs57@gmail.com"
        }
    },
    "paths": {
        "/users": {
            "post": {
                "tags": ["users"],
                "sumary": "criacao usuarios",
                "description": "Criacao de um novo usuario",
                "requestBody": {
                    "content":{
                        "application/json":{
                            "schema":{
                                "type":"object",
                                "properties": {
                                    "name": {
                                        "type": "string"    
                                    },
                                    "email":{
                                        "type": "string"
                                    }, 
                                    "senha":{
                                        "type": "string"
                                    }
                                },
                                "example":{
                                    "name": "userTest",
                                    "email": "user@test.com",
                                    "senha": "user#123.Test"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201":{
                        "description": "Conta Criada Com sucesso"
                    }, 
                    "401": {
                        "description": "uma conta com o email ja existe"
                    }
                }
            },
            "get": {
                "tags": ["users"],
                "sumary": "Login de usuario",
                "description": "verificação para ver se o usuario é ele mesmo para entrar no seus contatos",
                "requestBody": {
                    "content":{
                        "application/json":{
                            "schema":{
                                "email":{
                                    "type": "string"
                                },
                                "senha":{
                                    "type": "string"
                                },
                                "example":{
                                    "name":"user@test.com",
                                    "senha":"user#123.Test"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201":{
                        "description":"id_user:  15"
                    },
                    "401": {
                        "description": "usuario nao tem uma conta"
                    }
                }
            }
        }
    }
}