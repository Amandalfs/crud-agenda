    "paths": {
        "/users": {
            "post": {
                "tags": ["users"],
                "sumary": "criacao usuarios",
                "description": "Criacao de um novo usuario",
                "requestBody": {
                    "content":{
                        "application/json":{
                            "schema":{
                                "type":"object",
                                "properties": {
                                    "name": {
                                        "type": "string"    
                                    },
                                    "email":{
                                        "type": "string"
                                    }, 
                                    "senha":{
                                        "type": "string"
                                    }
                                },
                                "example":{
                                    "name": "userTest",
                                    "email": "user@test.com",
                                    "senha": "user#123.Test"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201":{
                        "description": "Conta Criada Com sucesso"
                    }, 
                    "401": {
                        "description": "uma conta com o email ja existe"
                    }
                }
            }
        }
    }
}